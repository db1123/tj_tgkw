  <select id="getCLassStudentlist" parameterType="fun.server.model.customQuery.classStudent.ClassStudentCS"
          resultType="fun.server.model.customQuery.classStudent.ClassStudentData">

    SELECT s.FKeyID,ifnull(s.FName,'') as FName,ifnull(s.FNo,'') as FNo,ifnull(s.FIDNumber,'') as FIDNumber,ifnull(s.FTel,'') as FTel,s.FSex
    FROM t_student s
    LEFT JOIN t_class_student cs
    ON s.FKeyID = cs.FStudentID
    <if test="CLassID != null and CLassID!='' and CLassID!='0' ">
      AND cs.FClassID = #{CLassID}
    </if>
    WHERE cs.FStudentID IS NULL
    <if test="FName != null and FName!=''">
      and s.FName like concat('%',#{FName},'%')
    </if>
    <if test="FTel != null and FTel!=''">
      and s.FTel like concat('%',#{FTel},'%')
    </if>
    <if test="FNo != null and FNo!=''">
      and s.FNo like concat('%',#{FNo},'%')
    </if>
    <if test=" FSex != null and FSex!=''">
      and s.FSex = #{FSex}
    </if>
    <if test="FIDNumber != null and FIDNumber!=''">
      and s.FIDNumber like concat('%',#{FIDNumber},'%')
    </if>
    <if test="orderBy != null and orderBy!=''">
      order by ${orderBy}
    </if>
  </select>

