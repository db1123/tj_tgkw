    </if>
  </select>
  <select id="getcourseEnrollmentCourseSelect" parameterType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentCourseCS" resultType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentCourseData">
    SELECT
    CONCAT(ttp.FName, '/',tc.FNo,'/',tc.FName) as FName
    ,FCOID,FCourseID
    FROM t_course_enrollment  tce
    LEFT JOIN t_course tc
    ON tce.FCourseID = tc.FKeyID
    LEFT JOIN t_training_program ttp
    ON tce.FCOID = ttp.FKeyID
    WHERE FIfPass = -1
    <if test="FOName != null and FOName!=''">
      and (ttp.FName like concat('%',#{FOName},'%') or tc.FNo like concat('%',#{FOName},'%') or tc.FName like concat('%',#{FOName},'%'))
    </if>
    <if test="orderBy != null and orderBy!=''">
      order by ${orderBy}
    </if>
    GROUP BY FCOID,FCourseID
  </select>

