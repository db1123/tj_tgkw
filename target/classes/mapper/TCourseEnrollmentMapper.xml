  <select id="getCourseEnrollmentDataList" parameterType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentCS" resultType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentData">
    SELECT
    tce.*
    ,IFNULL(ttp.FName,'') AS FTPName,IFNULL(tc.FName,'') AS FCourseName
    ,tc.FNo AS FCourseNo,tc.FType AS FLTypeID ,tc.FCNature AS FNatureID,tc.FEdition AS FCourseEdition,tc.FYWName
    ,tss.FNo AS FStudentNo,tss.FName AS FStudentName,tss.FSex AS FStudentSex,tss.FTel,tce.FCJType
    FROM t_course_enrollment tce
    LEFT JOIN t_training_program ttp
    ON tce.FCOID = ttp.FKeyID
    LEFT JOIN t_course tc
    ON tce.FCourseID = tc.FKeyID
    LEFT JOIN t_student tss
    ON tce.FStudentID = tss.FKeyID
    WHERE 1=1
    <if test="FStudentNo != null and FStudentNo!=''">
      and tss.FNo like concat('%',#{FStudentNo},'%')
    </if>

    <if test="FTPName != null and FTPName!=''">
      and ttp.FName like concat('%',#{FTPName},'%')
    </if>


    <if test="FSex != null and FSex!='' and FSex!= -1 ">
      and tss.FSex = #{FSex}
    </if>
    <if test="FLTypeID != null and FLTypeID!='' and FLTypeID!=0">
      and tc.FType = #{FLTypeID}
    </if>
    <if test="FNatureID != null and FNatureID!='' and FNatureID!=0">
      and tc.FCNature = #{FNatureID}
    </if>
    <if test="FCourseName != null and FCourseName!=''">
      and tc.FName like concat('%',#{FCourseName},'%')
    </if>
    <if test="FCourseNo != null and FCourseNo!=''">
      and tc.FNo like concat('%',#{FCourseNo},'%')
    </if>
    <if test="FTel != null and FTel!=''">
      and tss.FTel like concat('%',#{FTel},'%')
    </if>
    <if test="FStudentName != null and FStudentName!=''">
      and tss.FName like concat('%',#{FStudentName},'%')
    </if>

    <if test="FIfPass != null and FIfPass!='' and FIfPass!='-2'">
      and tce.FIfPass = #{FIfPass}
    </if>


    <if test="orderBy != null and orderBy!=''">
      order by ${orderBy}
    </if>
  </select>

  <select id="getCourseEnrollmentDataListmb" parameterType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentCS" resultType="fun.server.model.customQuery.courseEnrollment.courseEnrollmentData">
    SELECT
    tce.*
    ,IFNULL(ttp.FName,'') AS FTPName,IFNULL(tc.FName,'') AS FCourseName
    ,tc.FNo AS FCourseNo,tc.FType AS FLTypeID ,tc.FCNature AS FNatureID,tc.FEdition AS FCourseEdition,tc.FYWName
    ,tss.FNo AS FStudentNo,tss.FName AS FStudentName,tss.FSex AS FStudentSex,tss.FTel,tce.FCJType
    FROM t_course_enrollment tce
    LEFT JOIN t_training_program ttp
    ON tce.FCOID = ttp.FKeyID
    LEFT JOIN t_course tc
    ON tce.FCourseID = tc.FKeyID
    LEFT JOIN t_student tss
    ON tce.FStudentID = tss.FKeyID
    WHERE 1=1 and tce.FIfPass = -1

    <if test="FCOID != null and FCOID!='' and FCOID!=0">
