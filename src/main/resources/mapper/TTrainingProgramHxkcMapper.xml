  <select id="getCourseListopen" parameterType="fun.server.model.customQuery.trainingprogram.TrainingprogramkcCS"
          resultType="fun.server.model.customQuery.trainingprogram.TrainingprogramkcData">

    SELECT tc.FKeyID,ifnull(tc.FNo,'') as FNo,IFNULL(tc.FName,'') AS FName,IFNULL(tc.FYWName,'') AS FYWName,tc.FType AS FTypeID,tc.FCNature AS FNatureID
    ,tc.FCDATE ,tc.FUDATE,IFNULL(tcn.FName,'') AS FNatureName,tc.FEdition
    FROM t_course tc
    LEFT JOIN t_course_nature tcn
    ON tcn.FKeyID = tc.FCNature
    <choose>
      <when test="FType != null and FType!='' and FType == 1">
        LEFT JOIN t_training_program_hxkc  ttph
        ON tc.FKeyID = ttph.FCourseID
      </when>
      <otherwise>
        LEFT JOIN t_training_program_course_st  ttph
        ON tc.FKeyID = ttph.FCourseID
      </otherwise>
    </choose>
    <if test="FTPID != null and FTPID!='' and FTPID!='0' ">
      AND ttph.FTPID = #{FTPID}
    </if>
    WHERE ttph.FCourseID IS NULL AND tc.FState =2 AND tc.FValid = 1
    <if test="FName != null and FName!=''">
      and tc.FName like concat('%',#{FName},'%')
    </if>
    <if test="FNo != null and FNo!=''">
      and tc.FNo like concat('%',#{FNo},'%')
    </if>
    <if test="FLTypeID != null and FLTypeID!='' and FLTypeID!='0' ">
      AND tc.FType = #{FLTypeID}
    </if>
    <if test="FNatureID != null and FNatureID!='' and FNatureID!='0' ">
      AND tc.FCNature = #{FNatureID}
    </if>
    <if test="orderBy != null and orderBy!=''">
      order by ${orderBy}
    </if>
  </select>

